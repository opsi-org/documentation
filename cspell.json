{
	"version": "0.2",
	"dictionaryDefinitions": [
		{
			"name": "asciidoctor-words",
			"path": "./cspell/asciidoctor-words.txt",
			"addWords": false
		},
		{
			"name": "opsi-words",
			"path": "./cspell/opsi-words.txt",
			"addWords": false
		},
		{
			"name": "opsi-words-de",
			"path": "./cspell/opsi-words-de.txt",
			"addWords": false
		},
		{
			"name": "opsi-words-en",
			"path": "./cspell/opsi-words-en.txt",
			"addWords": false
		}
	],
	"languageSettings": [
		{
			"languageId": "*",
			"locale": "de",
			"dictionaries": ["asciidoctor-words", "opsi-words", "opsi-words-de"]
		},
		{
			"languageId": "*",
			"locale": "en",
			"dictionaries": ["asciidoctor-words", "opsi-words", "opsi-words-en"]
		}
	],
	"overrides": [
		{
			"language": "de",
			"filename": "docs/de/**"
		},
		{
			"language": "en",
			"filename": "docs/en/**"
		}
	],
	"ignorePaths": ["cspell/**", "cspell.json"],
	"ignoreRegExpList": [
		"documentation is owned by uib",
		"under the german creative commons",
		"xref:.*\\.adoc"
	],
	"words": [
	]
}